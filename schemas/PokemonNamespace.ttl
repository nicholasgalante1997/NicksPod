@prefix pokemon: <>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.

# Define the Pokemon Set class
pokemon:CardSet a rdfs:Class ;
    rdfs:label "Pokemon Card Set" ;
    rdfs:comment "A set datum containing a specific release of pokemon cards" .

pokemon:name a rdf:Property ;
    rdfs:domain pokemon:CardSet ;
    rdfs:label "Name" ;
    rdfs:comment "The set name" ;
    rdfs:range rdfs:Literal .

pokemon:id a rdf:Property ;
    rdfs:domain pokemon:CardSet ;
    rdfs:label "ID" ;
    rdfs:comment "The set ID" ;
    rdfs:range rdfs:Literal .

pokemon:release a rdf:Property ;
    rdfs:domain pokemon:CardSet ;
    rdfs:label "Release Date" ;
    rdfs:comment "The set release date" ;
    rdfs:range rdfs:Literal .

# Define the PokémonCard class
pokemon:PokemonCard a rdfs:Class ;
    rdfs:label "Pokémon Card" ;
    rdfs:comment "A card representing a Pokémon." .

# Define properties that belong to the class.
pokemon:type a rdf:Property ;
    rdfs:domain poke:PokemonCard ;
    rdfs:label "Type" ;
    rdfs:comment "The type of the Pokémon." ;
    rdfs:range rdfs:Literal .
    
pokemon:level a rdf:Property ;
    rdfs:domain pokemon:pokemonCard ;
    rdfs:label "Level" ;
    rdfs:comment "The level of the Pokémon." ;
    rdfs:range rdfs:Literal .

pokemon:hp a rdf:Property ;
    rdfs:domain pokemon:pokemonCard ;
    rdfs:label "HP" ;
    rdfs:comment "The hit points of the Pokémon." ;
    rdfs:range rdfs:Literal .

pokemon:attack a rdf:Property;
    rdfs:domain pokemon:pokemonCard ;
    rdfs:label "Attack";
    rdfs:comment "The attack of the Pokémon.";
    rdfs:range rdfs:Literal .

# Define a property to relate Pokémon cards to Pokémon sets
pokemon:inSet a rdf:Property ;
    rdfs:label "In Set" ;
    rdfs:comment "Indicates which set a Pokémon card belongs to." ;
    rdfs:domain pokemon:PokemonCard ;
    rdfs:range pokemon:CardSet .