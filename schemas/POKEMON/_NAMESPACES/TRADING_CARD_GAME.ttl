@prefix poke: <http://example.org/pokemon#>. # TODO this needs a real domain
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.

# Define the PokémonCard class
poke:PokemonCard a rdfs:Class ;
    rdfs:label "Pokémon Card" ;
    rdfs:comment "A card representing a Pokémon." .

# Define the PokémonSet class
poke:PokemonSet a rdfs:Class ;
    rdfs:label "Pokémon Set" ;
    rdfs:comment "A set of Pokémon cards." .

# Define properties for Pokémon cards
poke:id a rdf:Property ;
    rdfs:label "ID" ;
    rdfs:comment "The ID of the Pokémon card." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range rdfs:Literal .

poke:name a rdf:Property ;
    rdfs:label "Name" ;
    rdfs:comment "The name of the Pokémon." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range rdfs:Literal .

poke:supertype a rdf:Property ;
    rdfs:label "Supertype" ;
    rdfs:comment "The supertype of the Pokémon card." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range rdfs:Literal .

poke:subtypes a rdf:Property ;
    rdfs:label "Subtypes" ;
    rdfs:comment "The subtypes of the Pokémon card." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range rdfs:Literal .

poke:hp a rdf:Property ;
    rdfs:label "HP" ;
    rdfs:comment "The hit points of the Pokémon." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range rdfs:Literal .

poke:types a rdf:Property ;
    rdfs:label "Types" ;
    rdfs:comment "The types of the Pokémon." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range rdfs:Literal .

poke:evolvesFrom a rdf:Property ;
    rdfs:label "Evolves From" ;
    rdfs:comment "The Pokémon that this card evolves from." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range rdfs:Literal .

poke:abilities a rdf:Property ;
    rdfs:label "Abilities" ;
    rdfs:comment "The abilities of the Pokémon card." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range rdfs:Literal .

poke:attacks a rdf:Property ;
    rdfs:label "Attacks" ;
    rdfs:comment "The attacks of the Pokémon card." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range rdfs:Literal .

poke:weaknesses a rdf:Property ;
    rdfs:label "Weaknesses" ;
    rdfs:comment "The weaknesses of the Pokémon card." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range rdfs:Literal .

poke:retreatCost a rdf:Property ;
    rdfs:label "Retreat Cost" ;
    rdfs:comment "The retreat cost of the Pokémon card." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range rdfs:Literal .

poke:convertedRetreatCost a rdf:Property ;
    rdfs:label "Converted Retreat Cost" ;
    rdfs:comment "The converted retreat cost of the Pokémon card." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range rdfs:Literal .

poke:set a rdf:Property ;
    rdfs:label "Set" ;
    rdfs:comment "The set the Pokémon card belongs to." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range poke:PokemonSet .

poke:number a rdf:Property ;
    rdfs:label "Number" ;
    rdfs:comment "The number of the Pokémon card in the set." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range rdfs:Literal .

poke:artist a rdf:Property ;
    rdfs:label "Artist" ;
    rdfs:comment "The artist of the Pokémon card." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range rdfs:Literal .

poke:rarity a rdf:Property ;
    rdfs:label "Rarity" ;
    rdfs:comment "The rarity of the Pokémon card." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range rdfs:Literal .

poke:flavorText a rdf:Property ;
    rdfs:label "Flavor Text" ;
    rdfs:comment "The flavor text of the Pokémon card." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range rdfs:Literal .

poke:nationalPokedexNumbers a rdf:Property ;
    rdfs:label "National Pokédex Numbers" ;
    rdfs:comment "The national Pokédex numbers of the Pokémon." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range rdfs:Literal .

poke:legalities a rdf:Property ;
    rdfs:label "Legalities" ;
    rdfs:comment "The legalities of the Pokémon card." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range rdfs:Literal .

poke:images a rdf:Property ;
    rdfs:label "Images" ;
    rdfs:comment "The images of the Pokémon card." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range rdfs:Literal .

poke:tcgplayer a rdf:Property ;
    rdfs:label "TCGPlayer" ;
    rdfs:comment "The TCGPlayer information for the Pokémon card." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range rdfs:Literal .

poke:cardmarket a rdf:Property ;
    rdfs:label "CardMarket" ;
    rdfs:comment "The CardMarket information for the Pokémon card." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range rdfs:Literal .

# Define a property to relate Pokémon cards to Pokémon sets
poke:inSet a rdf:Property ;
    rdfs:label "In Set" ;
    rdfs:comment "Indicates which set a Pokémon card belongs to." ;
    rdfs:domain poke:PokemonCard ;
    rdfs:range poke:PokemonSet .
